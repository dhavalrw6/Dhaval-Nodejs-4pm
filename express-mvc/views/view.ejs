<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
    <table align="center" width="50%" border="1">
        <tr>
            <td>UserName</td>
            <td>Email</td>
            <td>Password</td>
            <td>Phone</td>
            <td>Action</td>
        </tr>
        <% data.map((val,idx)=>{ 
            let {username,email,password,phone,id} = val;
        %>
            <tr>
                <td><%= username %></td>
                <td><%= email %></td>
                <td><%= password %></td>
                <td><%= phone %></td>
                <td>
                    <a href="/deleteData/<%= id %>">Delete</a>
                    <a href="/editData/<%= id %>">Edit</a>
                </td>
            </tr>
        <% }) %>
    </table>
</body>
</html>